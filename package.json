    "type": "git",
    "url": "git://github.com/BizSim-Game-Studios/colyseus-monitor.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "colyseus",
    "multiplayer",
    "stats",
    "monitoring",
    "realtime",
    "websockets"
  ],
