{
  "name": "@bizsim-game-studios/colyseus-monitor",
  "version": "0.16.7",
  "description": "Web Monitoring Panel for Colyseus (BizSim fork)",
  "input": "./src/index.ts",
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    }
  },
  "scripts": {
    "start": "ts-node example/Server.ts",
    "build": "node build.mjs && npm run build:webpack",
    "build:backend": "node build.mjs",
    "build:webpack": "npx cross-env NODE_OPTIONS=--openssl-legacy-provider webpack --env.production",
    "prepublishOnly": "node build.mjs && npm run build"
  },
